        <macro key="baselinePainConstructB" value="E60A40E0-B954-4251-86AD-10E875E7FFDB"/>
        <macro key="baselinePainConstructC" value="410C8D80-B20D-4FB3-979A-E269506CE783"/>
        <macro key="baselinePainConstructD" value="ABF99F58-599B-4700-BC91-AD52FE81B18F"/>
